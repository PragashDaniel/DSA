class Compute {
    
    public static int missingNumber(int A[], int N)
    {
         // Your code goes here
         int sum1=0,sum2=0;
         for(int i=0;i<A.length;i++)
         {
             sum1+=A[i];
         }
         sum2=N*(N+1)/2;
         int sum=sum1>sum2?sum1-sum2:sum2-sum1;
         return sum;
         
    }
}
